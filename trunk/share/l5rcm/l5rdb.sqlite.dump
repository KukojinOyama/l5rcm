INSERT INTO "kata" VALUES(1132,'Striking as Air','Air',3,'striking_as_air','When in the Defense Stance your Armor TN is increased by your Air Ring');
INSERT INTO "kata" VALUES(1133,'Striking as Earth','Earth',3,'striking_as_earth','When in the Full Defense Stance you gain Reduction equal to your Earth Ring.  This is cumulative with other forms of Reduction');
INSERT INTO "kata" VALUES(1134,'Striking as Fire','Fire',3,'striking_as_fire','When in the Full Attack Stance you may add your Fire Ring to the total of one attack roll made per round');
INSERT INTO "kata" VALUES(1135,'Striking as Void','Void',3,'striking_as_void','When in the Center Stance your Armor TN is increased by your Void Ring');
INSERT INTO "kata" VALUES(1136,'Striking as Water','Water',4,'striking_as_water','When in the Attack Stance you may move 5 additional feet as a Free Action');
INSERT INTO "kata" VALUES(1137,'Balance The Elements Style','Void',3,'balance_the_elements_style','You may use your Void Ring in place of your Reflexes for your Initiative roll');
INSERT INTO "kata" VALUES(1138,'Breath of Wind Style','Air',3,'breath_of_wind_style','Your initiative Score increases by 2 during the Reactions Stage of each Combat Round. This effect stacks but disappears immediately if the kata is no longer in effect');
