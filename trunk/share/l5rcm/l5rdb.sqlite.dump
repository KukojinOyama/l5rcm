INSERT INTO "kata" VALUES(1140,'Hidden Blade Style','Air',4,'hidden_blade_style','Your Disarm attacks deal normal damage rather than 2k1. You may not increase your damage on these attacks via Raises (including from use of the Feint Maneuver)');
INSERT INTO "kata" VALUES(1141,'Indomitable Warrior Style','Earth',4,'indomitable_warrior_style','Reduce your TN penalties from Wound Ranks by your Earth Ring');
