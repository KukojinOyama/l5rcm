INSERT INTO "mastery_abilities" VALUES(176,3,NULL,'El terreno Moderado ya no entorpece tu movimiento, y moverte por terreno Difícil reduce el Anillo de Agua del personaje en 1 en lugar de en 2.');
INSERT INTO "mastery_abilities" VALUES(176,5,NULL,'El personaje ya no sufre penalizadores al movimiento sin importar el terreno.');
INSERT INTO "mastery_abilities" VALUES(176,7,NULL,'El personaje puede añadir 1,5 metros al total de una acción de Movimiento por Asalto, aunque no incrementa su capacidad de movimiento máxima durante ese Asalto.');
INSERT INTO "mastery_abilities" VALUES(177,5,'ma_battle_5','Añade tu rango en la Habilidad de Batalla al Valor de Iniciativa durante una escaramuza.');
INSERT INTO "mastery_abilities" VALUES(178,3,NULL,'Puedes mantener el resultado de la anterior tirada de Defensa/Reflejos en lugar de efectuar una nueva tirada si se mantiene en posición de Defensa Total durante varios Asaltos.');
INSERT INTO "mastery_abilities" VALUES(178,5,NULL,'El NO de armadura del personaje se considera 3 puntos superior al real en las posiciones de Defensa y Defensa Total.');
INSERT INTO "mastery_abilities" VALUES(178,7,NULL,'Se puede efectuar una única accion Simple en la posición de Defensa Total, pero no ataques.');
INSERT INTO "mastery_abilities" VALUES(179,3,NULL,'Se puede utilizar la posición de Ataque Total montado a caballo.');
INSERT INTO "mastery_abilities" VALUES(179,5,NULL,'Montar a caballo es una acción Simple, y desmontar es una acción Gratuita.');
INSERT INTO "mastery_abilities" VALUES(179,7,NULL,'Montar a caballo es una acción Gratuita.');
INSERT INTO "mastery_abilities" VALUES(180,5,NULL,'Bonificador de +1g0 al total de todas sus tiradas de Sigilo al aire libre en ambientes no urbanizados.');
INSERT INTO "mastery_abilities" VALUES(181,3,NULL,'Preparar una katana es una accion Gratuita.');
INSERT INTO "mastery_abilities" VALUES(181,5,NULL,'Durante un duelo de Iaijutsu, se obtiene un Aumento Gratuito en la tirada de Iaijutsu(Concentración)/Vacío durante la Fase de Concentración.');
INSERT INTO "mastery_abilities" VALUES(181,7,NULL,'Durante la Fase de Estudio de un duelo de Iaijutsu, el personaje obtiene un bonificador de +2g2 al resultado de todas sus tiradas de Concentración si su tirada de Estudio excede la de su oponente por 10 ó más, en lugar del +1g1 normal.');
INSERT INTO "mastery_abilities" VALUES(182,3,NULL,'El daño de todos los ataques sin armas se incrementa en +1g0.');
INSERT INTO "mastery_abilities" VALUES(182,5,NULL,'Usar Jiujitsu otorga un Aumento Gratuito para iniciar una Presa.');
INSERT INTO "mastery_abilities" VALUES(182,7,NULL,'El daño de todos los ataques sin armas se incrementa en +0g1, para un total de +1g1.');
INSERT INTO "mastery_abilities" VALUES(183,3,NULL,'Se puede utilizar armas de cadena para iniciar una presa.');
INSERT INTO "mastery_abilities" VALUES(183,5,NULL,'Los personajes que utilicen un arma de cadena obtienen un bonificador de +1g0 en cualquier tirada Enfrentada contra cualquier oponente que haya quedado Atrapado o Apresado a causa de su arma.');
INSERT INTO "mastery_abilities" VALUES(183,7,NULL,'El uso de un arma de cadena otorga un Aumento Gratuito para el uso de maniobras de Desarmar o Derribo.');
INSERT INTO "mastery_abilities" VALUES(184,3,NULL,'Los oponentes con un rango de Reducción ven este rango reducido en 2 puntos cuando sufran ataques con un arma pesada.');
