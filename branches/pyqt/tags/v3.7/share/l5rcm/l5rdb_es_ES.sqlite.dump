CREATE TABLE kata
        (uuid INTEGER PRIMARY KEY, name TEXT UNIQUE, ring VARCHAR,
         mastery INTEGER, rule VARCHAR, desc TEXT);
INSERT INTO "kata" VALUES(1131,'Golpe de Aire','Air',3,'striking_as_air','Mientras te encuentres en la posición de Defensa, tu NO de Armadura se 



incrementará en una cantidad igual a tu Anillo de Aire.');
INSERT INTO "kata" VALUES(1132,'Golpe de Tierra','Earth',3,'striking_as_earth','Mientras te encuentres en la posición de Defensa Total, obtienes un valor de 



Reducción igual a tu Anillo de Tierra (este efecto es acumulativo con otras fuentes 



de Reducción).');
INSERT INTO "kata" VALUES(1133,'Golpe de Fuego','Fire',3,'striking_as_fire','Mientras te encuentres en la posición de Ataque Total, puedes añadir tu 



Anillo de Fuego al resultado de una tirada de Ataque por Asalto.');
INSERT INTO "kata" VALUES(1134,'Golpe de Vacío','Void',3,'striking_as_void','Mientras te encuentres en la posición Centrada, tu NO de Armadura se 



incrementará en una cantidad igual a tu Anillo de Vacío.');
INSERT INTO "kata" VALUES(1135,'Golpe de Agua','Water',4,'striking_as_water','Mientras te encuentres en la posición de Ataque, podrás moverte 1,5 metros 



adicionales como una acción Gratuita.');
INSERT INTO "kata" VALUES(1136,'Puedes utilizar tu Anillo de Vacío en lugar de tus Reflejos para tus 



tiradas de Iniciativa.','Void',3,'balance_the_elements_style','Puedes utilizar tu Anillo de Vacio en lugar de tus Reflejos para tus tiradas de Iniciativa.');
INSERT INTO "kata" VALUES(1137,'Estilo del Hálito de Viento','Air',3,'breath_of_wind_style','Tu Valor de Iniciativa se incrementa en +2 durante la Fase de Reacción de 



cada Asalto de combate. Este efecto es acumulable pero desaparece de forma 



inmediata si se desactiva el kata.');
INSERT INTO "kata" VALUES(1138,'Estilo de la Desaparicion del Mundo','Fire',4,'disappearing_world_style','Escoge un oponente. Una vez por Asalto contra ese oponente, podrás utilizar 



tu Característica de Agilidad en lugar de tu Característica de Fuerza para las 



tiradas de daño.');
INSERT INTO "kata" VALUES(1139,'Estilo de la Espada Oculta','Air',4,'hidden_blade_style','Tus ataques de Desarmar causan el daño normal en lugar de 2g1. No puedes 



incrementar tu daño en estos ataques con el uso de Aumentos, ni siquiera con el uso 



de la maniobra de Finta.');
INSERT INTO "kata" VALUES(1140,'Estilo del Guerrero Indómito','Earth',4,'indomitable_warrior_style','Reduce los penalizadores al NO por Heridas en una cantidad igual a tu Anillo 



de Tierra.');
INSERT INTO "kata" VALUES(1141,'Estilo del Bosque de Hierro','Air',4,'iron_forest_style','Al utilizar una lanza o arma de asta, puedes utilizar tu Anillo de Aire en 



lugar de tu Agilidad para las tiradas de Ataque.');
INSERT INTO "kata" VALUES(1142,'Estilo de Hierro en las Montañas','Earth',3,'iron_in_the_Mountains_style','Puedes utilizar tu Anillo de Tierra en lugar de tu Anillo de Aire en la 



posición de Defensa.');
INSERT INTO "kata" VALUES(1143,'Estilo del Abandono Temerario','Fire',4,'reckless_abandon_style','Puedes añadir tu Anillo de Fuego a tu NO de Armadura mientras te encuentres 



en la posición de Ataque Total.');
INSERT INTO "kata" VALUES(1144,'Estilo de la Fuerza de las Armas','Water',4,'strength_in_arms_style','Una vez por asalto, al usar un arma pesada podrás utilizar tu Característica 



de Fuerza en lugar de tu Agilidad para la tirada de Ataque.');
INSERT INTO "kata" VALUES(1145,'Estilo de la Fuerza de la Pureza','Void',4,'strength_of_purity_style','Una vez por Asalto al hacer una tirada de daño, puedes tirar tu rango de 



Honor en lugar de la tirada normal de Fuerza + ND del arma. Guardas una cantidad de 



dados igual al ND del arma de la forma usual.');
INSERT INTO "kata" VALUES(1146,'Estilo de las Hojas Giratorias','Fire',5,'spinning_blades_style','Una vez por Asalto al usar un arma en cada mano, podrás efectuar la maniobra 



de Ataque Adicional por 3 aumentos en lugar de 5. El segundo ataque proviene del 



arma en tu mano torpe, y causara el daño del arma de la forma normal. No puedes 



incrementar el daño causado por este ataque con Aumentos.');
INSERT INTO "kata" VALUES(1147,'Estilo de la Amenaza Velada','Air',4,'veiled_menace_style','Una vez por Asalto, puedes sumar tu rango en la Habilidad de Sigilo a tu NO 



de Armadura para un único ataque.');
INSERT INTO "kata" VALUES(1148,'Fuerza del Cangrejo','Earth',3,'strength_of_the_crab','Mientras llevas Armadura y en posición de Ataque, tu armadura dará +2 



adicional a Reducción.');
INSERT INTO "kata" VALUES(1149,'Fuerza de la Grulla','Air',3,'strength_of_the_crane','Mientras luchas con una espada o lanza sumas una cantidad equivalente a tu 



Rango de Honor - 3 (mínimo 1) a tu NO de Armadura.');
INSERT INTO "kata" VALUES(1150,'Fuerza del Dragón','Fire',3,'strength_of_the_dragon','Mientras portas una katana en tu mano principal y un wakizashi en tu mano 



torpe, ganas un +3 a tu NO de Armadura.');
INSERT INTO "kata" VALUES(1151,'Fuerza del León','Water',3,'strength_of_the_lion','Una vez por Asalto durante la Fase de Reacciones, puedes sumar un +3 al Valor 



de Iniciativa de un aliado en una escaramuza. Este efecto es acumulable pero 



desaparece de forma inmediata si se desactiva el kata.');
INSERT INTO "kata" VALUES(1152,'Hijo de las Tormentas','Water',3,'son_of_storms','Mientras atacas con un arma pequeña a melee, cualquier Reducción que tenga un 



oponente se reduce en 1.');
INSERT INTO "kata" VALUES(1153,'Fuerza de la Mantis','Air',3,'strength_of_the_mantis','El penalizador de ataque para ataques a distancia disparados contra oponentes 



que se encuentran a rango de melee se reduce en 3.');
INSERT INTO "kata" VALUES(1154,'Danza de los Vientos','Air',3,'dance_of_the_winds','Mientras lleves un arma de asta, o una lanza, tu Valor de Iniciativa se 



incrementa en 3.');
INSERT INTO "kata" VALUES(1155,'Fuerza del Fénix','Void',3,'strength_of_the_phoenix','Una vez por Turno cuando declaras una Acción de Proteger, puedes incrementar 



el NO de Armadura de la persona que estás protegiendo en +3 Heridas.');
INSERT INTO "kata" VALUES(1156,'Fuerza del Escorpión','Fire',3,'strength_of_the_scorpion','Una vez por Turno, despues de haber realizado con éxito una Maniobra de 



Finta, incrementas en +3 Heridas.');
INSERT INTO "kata" VALUES(1157,'Fuerza del Unicornio','Water',3,'strength_of_the_unicorn','Mientras estés montado, tu montura gana un bonificador de +3 a su NO de 



Armadura y 3 adicional a la Reducción.');
INSERT INTO "kata" VALUES(1158,'Arte del Ninjutsu','Water',5,'art_of_ninjutsu','Una vez por Asalto, puedes calcular la distancia de una Acción de Movimiento 



como si tu Anillo de Agua fuera igual a tu Rango de Habilidad de Sigilo. Esto no 



cambia la distancia máxima que puedes moverte en un Asalto.');
INSERT INTO "kata" VALUES(1159,'Fuerza de la Araña','Earth',3,'strength_of_the_spider','Una vez por Asalto, si causas exitosamente al menos 15 Heridas a tu oponente, 



sufre un penalizador de -3 al total de todas sus tiradas en su próximo Turno.');
CREATE TABLE kiho
        (uuid INTEGER PRIMARY KEY, name TEXT UNIQUE, ring VARCHAR,
         mastery INTEGER, rule VARCHAR, tag VARCHAR, desc TEXT);
CREATE TABLE mastery_abilities
        (skill_uuid INTEGER, skill_rank INTEGER, rule TEXT, brief TEXT,
