INSERT INTO "mastery_abilities" VALUES(187,7,'ma_kyujutsu_7','Al utilizar cualquier arco, la Fuerza del arco se incrementa en 1.');
INSERT INTO "mastery_abilities" VALUES(188,3,NULL,'Durante el primer Asalto de una escaramuza, un personaje que lleve un arma de asta obtiene un bonificador de +5 a su Valor de Iniciativa. Este bonificador sólo dura el primer Asalto.');
INSERT INTO "mastery_abilities" VALUES(188,5,NULL,'Las tiradas de daño efectuadas con un arma de asta contra oponentes montados o significativamente más grandes se incrementan en +1g0.');
INSERT INTO "mastery_abilities" VALUES(188,7,NULL,'Se puede preparar el arma de asta como una acción Gratuita.');
INSERT INTO "mastery_abilities" VALUES(189,3,NULL,'Durante el primer asalto de una escaramuza, al llevar una lanza puedes ignorar 3 puntos de Reducción al efectuar ataques cuerpo a cuerpo contra su oponente.');
INSERT INTO "mastery_abilities" VALUES(189,5,NULL,'Los ataques a distancia efectuados con una lanza incrementan su alcance en 1,5 metros.');
INSERT INTO "mastery_abilities" VALUES(189,7,NULL,'Preparar una lanza como acción Gratuita.');
INSERT INTO "mastery_abilities" VALUES(190,3,NULL,'Los modificadores de armaduras ya no se doblan contra ataques con bastones.');
