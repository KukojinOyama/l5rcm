CREATE TABLE effects
        (uuid INTEGER PRIMARY KEY, tag VARCHAR UNIQUE, desc TEXT);
INSERT INTO "effects" VALUES(801,'blowgun_special_rule','Triplica el NO de armadura de un oponente procedente de armaduras. El incremento al daño proveniente de la Habilidad de Ninjutsu aumenta el daño de la cerbatana a 1g1 a rango 3 y a 2g1 a rango 7. Recargar una cerbatana es una acción gratuita.');
INSERT INTO "effects" VALUES(802,'daikyu_rule','Incrementa el NO de todas las tiradas de ataque en +10 si se utiliza a pie.');
INSERT INTO "effects" VALUES(803,'dbl_opp_armor_rule','Dobla el bonificador al NO de armadura proveniente de la armadura de un oponente.');
INSERT INTO "effects" VALUES(804,'may_break_25w_rule','Si inflinge más de 25 heridas en un único ataque, se rompe.');
INSERT INTO "effects" VALUES(805,'may_break_30w_rule','Si inflinge más de 30 heridas en un único ataque, se rompe.');
INSERT INTO "effects" VALUES(806,'throw_20f_rule','Puede lanzarse como arma arrojadiza hasta a 6 metros de distancia.');
INSERT INTO "effects" VALUES(807,'throw_25f_rule','Puede lanzarse con precisión hasta a 7,5 metros.');
INSERT INTO "effects" VALUES(808,'throw_50f_rule','Puede lanzarse con precisión hasta a 15 metros.');
INSERT INTO "effects" VALUES(809,'lance_special_rule','El ND listado únicamente se aplica si la lanza de caballería se usa montado y sólo se utiliza para efectuar un ataque directamente después de una acción de Movimiento. Bajo otras circunstancias, su ND es de 1g2. Utilizar la lanza de caballería en combate cuerpo a cuerpo sin una acción de Movimiento incrementa el NO de todas las tiradas de ataque en +5 a caballo y en +10 a pie. Una lanza de caballería se hace astillas si causa más de 30 Heridas en un ataque.');
INSERT INTO "effects" VALUES(810,'yari_special_rule','El yari puede lanzarse hasta 9 metros, y su ND al lanzarse es de 1g2.');
INSERT INTO "effects" VALUES(811,'tsubute_special_rule','Un Tsubute puede lanzarse hasta a 9 metros. A causa de su pequeño tamaño, los tsubute no añaden la Fuerza del lanzador a su ND al ser usadas como armas a distancia, y sus dados de daño no son relanzables.');
INSERT INTO "effects" VALUES(812,'void_rise_dr_rule','Un personaje puede gastar 1 punto de Vacío para incrementar el resultado de una tirada de daño utilizando una katana en +1g1 (normalmente no es posible gastar puntos de Vacío para mejor las tiradas de daño). Sólo es posible gastar 1 punto de Vacío por asalto de  esta forma, incluso si el personaje tiene la capacidad de gastar más de 1 punto de Vacío por Asalto.');
INSERT INTO "effects" VALUES(813,'ninjato_special_rule','El ninja-to es un arma mediana, pero se trata como un arma pequeña a la hora de ocultarla. Si un ninja-to causa mas de 40 Heridas en un único ataque, su hoja se rompe.');
INSERT INTO "effects" VALUES(814,'keep_grapple_rule','Puede utilizarse para comenzar o mantener una Presa.');
INSERT INTO "effects" VALUES(815,'no_shoot_horseback_rule','Incrementa el NO de todas las tiradas de ataque en +10 si se utiliza a caballo.');
INSERT INTO "effects" VALUES(816,'light_armor_rule','Llevar armadura ligera incrementa el NO de todas las tiradas de Habilidad de Atletismo y de Sigilo en +5.');
INSERT INTO "effects" VALUES(817,'heavy_armor_rule','Llevar armadura pesada incrementa el NO de todas las tiradas de Habilidad en las que se utilice la Agilidad o los Reflejos en +5.');
INSERT INTO "effects" VALUES(818,'riding_armor_rule','+12 NO a caballo. La armadura de jinete cuenta como una armadura pesada para cualquier efecto que se especifique tipo de Armadura, incluyendo Técnicas, Ventajas, etc. Llevar armadura de jinete incrementa el NO de todas las tiradas de Agilidad y Reflejos en +5 a menos que el portador este montado a caballo.');
INSERT INTO "effects" VALUES(819,'ignore_armor_rule','Ignora el bonificadr al NO de Armadura proveniente de armaduras.');
INSERT INTO "effects" VALUES(820,'flesh_cutter_rule','Dobla el bonificador al NO de Armadura proveniente de armaduras, 1/2 alcance.');
INSERT INTO "effects" VALUES(821,'humming_bulb_rule','Lanza un potente sonido sibilante.');
INSERT INTO "effects" VALUES(822,'rope_cutter_rule','2 Aumentos Gratuitos para ataques apuntados contra objetos inanimados, 1/2 alcance.');
